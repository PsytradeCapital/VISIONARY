{
  "expo": {
    "name": "Visionary",
    "slug": "visionary-ai-scheduler",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.visionary.aischeduler",
      "buildNumber": "1"
    },
    "android": {
      "package": "com.visionary.aischeduler",
      "versionCode": 1,
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "NOTIFICATIONS"
      ]
    },
    "plugins": [
      "expo-notifications",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Visionary to access your camera to capture documents and images."
        }
      ],
      [
        "expo-av",
        {
          "microphonePermission": "Allow Visionary to access your microphone for voice input."
        }
      ]
    ],
    "scheme": "visionary",
    "extra": {
      "eas": {
        "projectId": "07a5735e-5110-40b1-9cc4-fb3ac0f4c193"
      }
    }
  }
}